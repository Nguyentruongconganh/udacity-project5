{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "book",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "publicDate": {
      "type": "string"
    },
    "myFavorite": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "title": {
      "type": "string"
    }
  },
  "required": ["name"],
  "additionalProperties": false
}
